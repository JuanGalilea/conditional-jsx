{
  "name": "conditional-jsx",
  "version": "1.0.6",
  "description": "React components for conditional statements.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanGalilea/conditional-jsx.git"
  },
  "keywords": [
    "react"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "vitest run",
    "watch": "vitest watch",
    "build": "npx tsc -p ."
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "require": "ts-node/register"
  },
  "author": "JuanGalilea",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/react": "^18.0.31",
    "chai": "^4.3.4",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^7.25.0",
    "lint-staged": "^10.5.4",
    "mocha": "^10.2.0",
    "prettier": "^2.2.1",
    "react-dom": "^18.2.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.8.1",
    "typescript": "^4.2.4",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
